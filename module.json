{
  "id": "nyquistt-helpers",
  "title": "Nyquistt Helpers Repo",
  "description": "<p>A simple module that provides helper functions and macros for FoundryVTT.</p>",
  "authors": [
    {
      "name": "Nyquist",
      "flags": {}
    }
  ],
  "version": "0.1.1",
  "compatibility": {
    "minimum": "13",
    "verified": "13"
  },
  "scripts": [
    "scripts/app-choose.js"
  ],
  "styles": [
    {
      "src": "styles/app-choose.css"
    }
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json",
      "flags": {}
    },
    {
      "lang": "it",
      "name": "Italian",
      "path": "lang/it.json",
      "flags": {}
    }
  ],
  "packs": [
    {
      "name": "journal-entries",
      "label": "Nyquistt Helpers Journal Entries",
      "path": "packs/journal-entries",
      "type": "JournalEntry",
      "ownership": {
        "PLAYER": "OBSERVER",
        "ASSISTANT": "OWNER"
      }
    },
    {
      "name": "macros",
      "label": "Nyquistt Helpers Macros",
      "path": "packs/macros",
      "type": "Macro",
      "ownership": {
        "PLAYER": "OBSERVER",
        "ASSISTANT": "OWNER"
      }
    },
    {
      "name": "macros-private",
      "label": "Nyquistt Helpers Macros Private",
      "path": "packs/macros-private",
      "type": "Macro",
      "ownership": {
        "PLAYER": "NONE",
        "ASSISTANT": "OWNER"
      }
    }
  ],
  "packFolders": [
    {
      "name": "Nyquistt Helpers",
      "color": "#c1a030",
      "packs": [
        "journal-entries",
        "macros",
        "macros-private",
        "journals-private"
      ],
      "folders": []
    }
  ]
}